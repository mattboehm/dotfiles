compiler pylint
setlocal tabstop=4
setlocal softtabstop=4
setlocal shiftwidth=4
setlocal textwidth=110
setlocal smarttab
setlocal expandtab
setlocal nowrap
set smartindent cinwords=if,elif,else,for,while,with,try,except,finally,def,class

inoremap <buffer> \[ [""]<Esc>hi
inoremap <buffer> ; _
inoremap <buffer> \; ;
